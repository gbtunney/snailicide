{
  "tags": {
  "allowUnknownTags": true,
  "dictionaries": ["jsdoc","closure"]
},
  "source": {
    "include": ["src/scripts"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "verbose": true,
    "destination": "public",
    "recurse": true,
    "readme": "README.md",
    "template": "templates/docdash"
  }
}
