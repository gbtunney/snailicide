{% assign settingsDataDefault = section.settings %}

{% if _settings.icon_path  contains ".svg" %}{{ _settings.icon_path  | asset_url }}{% else %}_settings.icon_path{% endif %}
<gbt-kabob
        {% if  _settings.content != null and  _settings.content != "" %}
            content="{{  _settings.content }}"
        {% endif %}
        class="{{ _settings.css_classes | default: settingsDataDefault.css_classes }}"
        style="{% if _settings.color_toggle == true %}color: {{ _settings.icon_color | default: settingsDataDefault.iconify_color }};{% endif %}"
        path="{% if _settings.icon_path  contains ".svg" %}{{ _settings.icon_path  | asset_url }}{% else %}{{ _settings.icon_path }}{% endif %}"
></gbt-kabob>
{% assign _settings = null %}

